+(SearchData*) initWithSectionType:(HDAModelType)secType RowArray:(NSMutableArray*)arrRows;
-(NSDictionary*) dictionaryRepresentation;
@end

@protocol SearchDataManagerDelegate <NSObject>
- (void) searchData:(SearchDataManager*)searchData didFindDataArray:(NSMutableArray*)arrSearchedData;
@end

